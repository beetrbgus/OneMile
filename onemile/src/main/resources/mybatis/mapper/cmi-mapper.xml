<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cmi">
	
	<!-- 시퀀스 번호 가져오기 -->
	<select id="sequence">
		select cmi_seq.nextval from dual
	</select>
	
	<!-- 사진 추가 -->
	<insert id="insert" parameterType="commuImageDto">
		insert into cmi
		values(
			#{cmiNo}, #{imageNo}, #{commuNo}
		)
	</insert>
</mapper>